{
  "name": "Dracula",
  "author": "Zeno Rocha",
  "version": "1.0.0",
  "description": "A dark theme for many editors, shells, and more",
  "type": "dark",
  "colors": {
    "primary": "#BD93F9",
    "primaryHover": "#9580D3",
    "secondary": "#FF79C6",
    "accent": "#50FA7B",
    "background": "#282A36",
    "surface": "#343746",
    "headerBg": "#21222C",
    "foreground": "#F8F8F2",
    "textPrimary": "#F8F8F2",
    "textSecondary": "#E0E0E0",
    "textMuted": "#6272A4",
    "border": "#44475A",
    "borderLight": "#3A3C4E"
  },
  "shadows": {
    "sm": "0 1px 3px rgba(0, 0, 0, 0.4)",
    "md": "0 4px 6px rgba(0, 0, 0, 0.3)",
    "lg": "0 10px 15px rgba(0, 0, 0, 0.3)"
  },
  "radius": {
    "default": "1.3em",
    "sm": "0.5em"
  }
